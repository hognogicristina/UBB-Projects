; PROBLEM AREA

; Write a function to sort a linear list without keeping the double values.

; (5 1 3 4 2 1 3) -> (1 2 3 4 5)
; (5 4 3 2 1) -> (1 2 3 4 5)
; (5 5 4 4 3 3 2 1 2 1) -> (1 2 3 4 5)
; (1 3 2 1 5 3 1 4) -> (1 2 3 4 5)

; -----------------------------------------------------------------------------------------------------

; CODE AREA

; for command line: (mySort '(5 1 3 4 2 1 3)) => (1 2 3 4 5)
(defun myInsert (l e)
    (cond ((null l) (list e))                           
          ((= (car l) e) l) 
          ((< e (car l)) (cons e l))
          (t (cons (car l) (myInsert(cdr l) e)))))

(defun mySort (l)
    (cond ((null l) nil)
          (t (myInsert (mySort (cdr l)) (car l)))))

; -----------------------------------------------------------------------------------------------------

; TESTS AREA

(defun myTest ()
    (and 
        (equal (mySort '(5 1 3 4 2 1 3)) '(1 2 3 4 5 6))
        (equal (mySort '(5 4 3 2 1)) '(1 2 3 4 5))
        (equal (mySort '(5 5 4 4 3 3 2 1 2 1)) '(1 2 3 4 5))
        (equal (mySort '(1 3 2 1 5 3 1 4)) '(1 2 3 4 5))))

